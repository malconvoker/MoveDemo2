			"eventType": "function-block",
			"conditions": [],
			"actions": [
				{
					"type": "comment",
					"text": "Returns distance between two tiles (diagonals =1)"
				},
				{
					"id": "set-text",
					"objectClass": "TXTmovedistance",
					"sid": 551967756646411,
					"disabled": true,
					"parameters": {
						"text": "abs((CursorT.X-PlayerBase.X)/Tile) & \", \" & abs((CursorT.Y-PlayerBase.Y)/Tile) & \": \" & max(abs((CursorT.X-PlayerBase.X)/Tile), abs((CursorT.Y-PlayerBase.Y)/Tile))"
					}
				},
				{
					"id": "set-function-return-value",
					"objectClass": "Functions",
					"sid": 262867071896114,
					"disabled": true,
					"parameters": {
						"value": "max(abs(floor((X1-X2)/Tile)), abs(floor((Y1-Y2)/Tile)))"
					}
				},
				{
					"type": "comment",
					"text": "Try without rounding down?"
				},
				{
					"id": "set-function-return-value",
					"objectClass": "Functions",
					"sid": 196334819418125,
					"parameters": {
						"value": "max(abs((X1-X2)/Tile), abs((Y1-Y2)/Tile))"
					}
				}
			],
			"sid": 749858027451949
